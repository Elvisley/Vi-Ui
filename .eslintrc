{
  "extends": [
    "plugin:prettier/recommended",
    "airbnb-base",
    "plugin:vue/strongly-recommended"
  ],
  "plugins": [
    "vue",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error", {
      "singleQuote": true,
      "trailingComma": "all",
      "arrowParens": "avoid",
      "jsxBracketSameLine": false
    }],
    "arrow-parens": ["error", "as-needed"]
  }
}
